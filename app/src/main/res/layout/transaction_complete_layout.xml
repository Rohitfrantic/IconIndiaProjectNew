<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:CustomTextView="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_marginTop="@dimen/dp_55"
    android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="@dimen/dp_8"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:background="#ffffff"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_60"
            android:layout_marginLeft="@dimen/dp_5"
            android:layout_marginTop="@dimen/dp_8"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginLeft="@dimen/dp_6">
                <com.frantic.icon_india.project.utills.CustomTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_8"
                    CustomTextView:textStyle="8"
                    android:textColor="@color/black"
                    android:text="Transaction Id"
                    android:textSize="@dimen/dp_16"
                    />
                <com.frantic.icon_india.project.utills.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0897098968"
                    CustomTextView:textStyle="1"
                    android:layout_marginTop="@dimen/dp_4"
                    android:id="@+id/profile_married"
                    android:textSize="@dimen/dp_16"
                    />
            </LinearLayout>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_marginTop="@dimen/dp_8"
            android:layout_height="@dimen/dp_12"
            android:alpha="0.4"
            android:background="#e7e3e3"
            />
        <com.frantic.icon_india.project.utills.CustomTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:layout_marginLeft="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_8"
            CustomTextView:textStyle="8"
            android:textColor="@color/black"
            android:text="DTH Recharged"
            android:textSize="@dimen/dp_16"
            />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_60"
            android:layout_marginLeft="@dimen/dp_5"
            android:layout_marginTop="@dimen/dp_8"
            android:layout_marginBottom="@dimen/dp_5"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="@dimen/dp_40"
                android:layout_height="@dimen/dp_40"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/dp_10"
                android:src="@drawable/dashboard_icon" />
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_20"
                android:orientation="vertical">
                <com.frantic.icon_india.project.utills.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_8"
                    CustomTextView:textStyle="8"

                    android:textColor="@color/black"
                    android:id="@+id/fuck_emailid"
                    android:text="Airtel Digital Tv"
                    android:textSize="@dimen/dp_16"
                    />
                <com.frantic.icon_india.project.utills.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="97677897778"
                    android:id="@+id/profile_ankur"
                    CustomTextView:textStyle="1"
                    android:layout_marginTop="@dimen/dp_4"
                    android:textSize="@dimen/dp_14"
                    />
            </LinearLayout>



            <com.frantic.icon_india.project.utills.CustomTextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_8"
                android:gravity="right"
                android:textColor="@color/black"
                CustomTextView:textStyle="8"
                android:text="Rs. 335"
                android:textSize="@dimen/dp_18"
                android:layout_marginRight="@dimen/dp_5"
                android:id="@+id/fuck_number"
                />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_marginTop="@dimen/dp_8"
            android:layout_height="@dimen/dp_12"
            android:alpha="0.4"
            android:background="#e7e3e3"
            />



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_6"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_60"
                    android:layout_marginLeft="@dimen/dp_5"
                    android:layout_marginTop="@dimen/dp_8"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="@dimen/dp_35"
                        android:layout_height="@dimen/dp_35"
                        android:layout_gravity="center"
                        android:src="@drawable/bank_deposit" />
                <com.frantic.icon_india.project.utills.CustomTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    CustomTextView:textStyle="8"
                    android:layout_gravity="center"
                    android:text="Issue with Transaction ??"
                    android:textSize="@dimen/dp_15"
                    android:textStyle="normal"
                    android:layout_marginLeft="@dimen/dp_15"
                    android:id="@+id/fuck_number1"
                    />
                </LinearLayout>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_marginTop="@dimen/dp_8"

            android:layout_height="@dimen/dp_12"
            android:alpha="0.4"
            android:background="#e7e3e3"
            />

    </LinearLayout>
</LinearLayout>